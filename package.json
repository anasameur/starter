    "jest": "^24.1.0",
    "jest-html-reporter": "^2.4.4",
    "jest-junit": "^6.3.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-notifier": "^5.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^3.1.0",
    "portfinder": "^1.0.20",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
