{
  "name": "app-starter",
  "version": "1.0.0",
  "description": "A Vue.js web-app starter project",
  "contributors": [
    "Anas Ameur <anas.ameur@engie.com>",
    "Vincent Guillou <vincent.guillou@engie.com>",
    "Jean-Nicolas Marnet <jean-nicolas.marnet@engie.com>"
  ],
  "private": true,
  "scripts": {
    "dev": "npm run format && webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "format": "prettier --write \"**/**.+(js|json|vue|md)\"",
    "lint": "eslint --ext .js,.vue src",
    "build": "node build/build.js",
    "test": "npm run lint && NODE_ENV=test jest",
    "open:coverage": "open ./__tests__/test-reports/coverage/lcov-report/index.html",
    "open:test-report": "open ./__tests__/test-reports/report.html",
    "open:report": "open ./report/webpack-bundle-analysis.html"
  },
  "dependencies": {
    "@okta/okta-vue": "1.0.1",
    "axios": "~0.18.0",
    "locale2": "~2.3.1",
    "lodash": "~4.17.11",
    "material-design-icons-iconfont": "~4.0.5",
    "vue": "~2.6.6",
    "vue-i18n": "^8.8.2",
    "vue-perfect-scrollbar": "~0.1.0",
    "vue-router": "~3.0.2",
    "vuetify": "^1.5.2",
    "vuex": "~3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "@types/jest": "^24.0.6",
    "@vue/babel-helper-vue-jsx-merge-props": "1.0.0-beta.2",
    "@vue/babel-plugin-transform-vue-jsx": "1.0.0-beta.2",
    "autoprefixer": "^9.4.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
